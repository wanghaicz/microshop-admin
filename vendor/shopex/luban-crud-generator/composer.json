{
    "name": "shopex/luban-crud-generator",
    "license": "MIT",
    "description": "Laravel 5 CRUD Generator",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator"
    ],
    "authors": [
        {
            "name": "shopex",
            "email": "dev@shopex.cn"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.6",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Shopex\\CrudGenerator\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shopex\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
